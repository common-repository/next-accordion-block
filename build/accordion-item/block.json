{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "nextblocks/accordion-item",
  "version": "1.0.0",
  "title": "Accordion Item",
  "description": "Single Item for Accordion",
  "category": "next-blocks",
  "parent": [
    "nextblocks/accordion"
  ],
  "supports": {
    "html": false,
    "anchor": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "source": "html",
      "selector": "p",
      "default": "Accordion Title Here"
    },
    "itemId": {
      "type": "string"
    }
  },
  "textdomain": "next-accordion-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}